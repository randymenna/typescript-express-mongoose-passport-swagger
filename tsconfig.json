{
    "compilerOptions": {
      "module": "commonjs",
      "esModuleInterop": true,
      "target": "es6",
      "noImplicitAny": true,
      "moduleResolution": "node",
      "sourceMap": true,
      "outDir": "dist",
      "baseUrl": ".",
      "importsNotUsedAsValues": "preserve",
      "paths": {
        "*": [
          "./*",
          "../node_modules/*"
        ],
        "src/*": [
          "./src/*"
        ],
        "@passport/*": [
          "./.src/middleware/passport/strategies/*"
        ]
      },
        "lib": [
          "es6"
        ]
    },
    "include": [
        "src/**/*"
    ]
}
